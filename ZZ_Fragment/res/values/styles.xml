<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="AppTheme" parent="android:Theme.Holo.Light.DarkActionBar">
        <!-- API 14 theme customizations can go here. -->
    </style>

    <style name="content_text">
        <item name="android:textSize">@dimen/content</item>
        <item name="android:textColor">@color/content</item>
    </style>

    <style name="list_footer_text">
        <item name="android:textSize">@dimen/list_footer_text</item>
        <item name="android:textColor">@color/content</item>
        <item name="android:textStyle">normal</item>
        <item name="android:typeface">sans</item>
    </style>

    <style name="list_user_profile_name">
        <item name="android:textSize">@dimen/list_user_profile_name</item>
        <item name="android:textColor">@color/content</item>
        <item name="android:textStyle">normal</item>
        <item name="android:typeface">sans</item>
    </style>

</resources>